POST http://localhost:19121/collections
{
  "collection_name": "art",
  "dimension": 1000
}


CREATE INDEX
POST http://localhost:19121/collections/art/indexes
{
  "index_type": "IVF_SQ8"
}


CREATE

POST http://localhost:19121/collections/test/vectors
{
  "vectors": [[0,0,1], [1,0,0], [0,1,0], [0.01,0,0], [0,0,0.01], [1,0.02,0], [1,0,0.2], [0,0.01,0.03], [0,0,0.04]],
